<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper 
    PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
    "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
   <!-- 
   -->

<mapper namespace="sms">
	<parameterMap id="sms" type="com.sw.plugins.clientcenter.member.generateaccount.entity.SmsSender"/>
	<!-- 短信发送 -->
	<insert id="insert" parameterMap="sms">
		insert into sm_send_sm_list
		<trim prefix="(" prefixOverrides="," suffix=")">
			<if test="serviceID !=null">,ServiceID</if>
			<if test="smContent !=null">,SMContent</if>
			<if test="sendTarget != null">,SendTarget</if>
			<if test="priority !=null">,Priority</if>
			<if test="recompleteTimeBegin !=null">,RCompleteTimeBegin</if>
			<if test="recompleteTimeEnd !=null">,RCompleteTimeEnd</if>
			<if test="recompleteHourBegin !=null">,RCompleteHourBegin</if>
			<if test="recompleteHourEnd !=null">,RCompleteHourEnd</if>
			<if test="rodeBy !=null">,RoadBy</if>
		</trim>
		values
		<trim prefix="(" prefixOverrides="," suffix=")">
			<if test="serviceID !=null">,#{serviceID}</if>
			<if test="smContent !=null">,#{smContent}</if>
			<if test="sendTarget != null">,#{sendTarget}</if>
			<if test="priority !=null">,#{priority}</if>
			<if test="recompleteTimeBegin !=null">,#{recompleteTimeBegin}</if>
			<if test="recompleteTimeEnd !=null">,#{recompleteTimeEnd}</if>
			<if test="recompleteHourBegin !=null">,#{recompleteHourBegin}</if>
			<if test="recompleteHourEnd !=null">,#{recompleteHourEnd}</if>
			<if test="rodeBy !=null">,#{rodeBy}</if>
		</trim>
	</insert>
	
</mapper>
